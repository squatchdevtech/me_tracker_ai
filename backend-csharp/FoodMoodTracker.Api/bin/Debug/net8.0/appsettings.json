{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=3306;Database=food_mood_tracker;User=root;Password=yourpassword;"
  },
  "Jwt": {
    "Secret": "your-super-secret-jwt-key-here-change-in-production",
    "Issuer": "FoodMoodTracker",
    "Audience": "FoodMoodTracker",
    "ExpiresInDays": "7"
  },
  "Cors": {
    "Origin": "http://localhost:4200"
  },
  "RateLimiting": {
    "WindowMs": 900000,
    "MaxRequests": 100
  }
}
